    "dotenv-safe": "^8.2.0",
    "e": "0.0.4",
    "express": "4.16.3",
    "http-proxy-middleware": "^2.0.1",
    "ip": "1.1.5",
    "minimist": "1.2.0"
  },
  "devDependencies": {
    "eslint": "4.19.1",
    "null-loader": "0.1.1",
    "plop": "2.0.0",
    "shelljs": "^0.8.1"
