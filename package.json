{
  "name": "server-boilerplate",
  "version": "1.0.0",
  "description": "A minimal server boilerplate for NodeJs.",
  "repository": {
    "type": "git",
    "url": "git://github.com/rodrigof23/node-server.git"
  },
  "engines": {
    "npm": ">=3",
    "node": ">=5"
  },
  "author": "Rodrigo Freitas",
  "scripts": {
    "start": "cross-env NODE_ENV=production node index.js",
